	{
		"manifest_version": 3,
		"name": "chibisafe Uploader",
		"description": "Uploader for chibisafe",
		"version": "2022.10.10.0",
		"version_name": "Holo",
		"minimum_chrome_version": "93.0",
		"options_ui": {
			"page": "pages/settings/settings.html",
			"open_in_tab": true
		},
		"icons": {
			"48": "images/logo-48x48.png",
			"128": "images/logo-128x128.png"
		},
		"permissions": [
			"contextMenus",
			"storage",
			"notifications",
			"scripting",
			"declarativeNetRequest"
		],
		"host_permissions": [
			"<all_urls>"
		],
		"background": {
			"service_worker": "js/background.js",
			"type": "module"
		}
	}
