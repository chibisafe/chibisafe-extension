{
	"manifest_version": 3,
	"name": "chibisafe Uploader",
	"description": "Uploader for chibisafe",
	"version": "2023.9.9.0",
	"version_name": "2.0.1",
	"minimum_chrome_version": "93.0",
	"options_ui": {
		"page": "settings.html",
		"open_in_tab": true
	},
	"icons": {
		"48": "images/logo-48x48.png",
		"128": "images/logo-128x128.png"
	},
	"permissions": [
		"contextMenus",
		"storage",
		"notifications",
		"scripting",
		"declarativeNetRequest"
	],
	"host_permissions": [
		"<all_urls>"
	],
	"background": {
		"service_worker": "js/background.mv3.js",
		"type": "module"
	}
}
